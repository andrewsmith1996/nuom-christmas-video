<?xml version='1.0' encoding='utf-8'?>
<widget id="co.arcweb.chappiecast" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <allow-intent href="market:*" />
    <preference name="AllowInlineMediaPlayback" value="true" />
    <name>ChappieCast</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="max@arcweb.co" href="http://arcweb.co">
        Max Furman
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="web_hi_res_512.png" />
    <preference name="Fullscreen" value="true" />
    <preference name="Orientation" value="portrait" />
</widget>
